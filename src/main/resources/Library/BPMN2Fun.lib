fun evaluateP(functions){
    var leftT = left(functions);
    var rightT = right(functions);

    var leftF = right(leftT);
    var rightF = right(rightT);

    leftF(left(leftT));
    rightF(left(rightT));

    print "Finish Parallel running";
}


